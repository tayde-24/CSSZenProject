{
  "name": "style-guide-template",
  "version": "1.0.0",
  "description": "Boilerplate for a StyleGuide template based off of the CodyHouse template",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel src/index.html -d build/",
    "prebuild": "rimraf dist/*",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html -d build/"
  },
  "author": "Shane T",
  "license": "GPL-2.0",
  "devDependencies": {
    "parcel-bundler": "^1.12.0",
    "postcss": "^8.3.5",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.0",
    "sass": "^1.23.0"
  },
  "dependencies": {
    "autoprefixer": "^10.2.6",
    "js": "^0.1.0"
  }
}
